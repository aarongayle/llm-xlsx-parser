{
  "name": "llm-xlsx-parser",
  "type": "module",
  "version": "0.1.2",
  "description": "Convert XLSX files into LLM-friendly formats with visual, CSV, and structured record processing",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "default": "./index.js"
    }
  },
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "test:validate": "node test/validate-specific-values.js",
    "test:demo": "node test/demo.js",
    "example": "node example.js",
    "postinstall": "playwright install chromium"
  },
  "keywords": [
    "xlsx",
    "excel",
    "llm",
    "ai",
    "analysis",
    "gemini",
    "image",
    "conversion",
    "spreadsheet"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@google/genai": "^1.10.0",
    "canvas": "^2.11.2",
    "dotenv": "^17.2.0",
    "exceljs": "^4.4.0",
    "playwright": "^1.40.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "vitest": "^1.6.1"
  }
}
